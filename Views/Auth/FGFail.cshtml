@model ABAC.DTO.BaseDTO
@{
    ViewData["Title"] = "Reset Password Fail";
    Layout = "_LayoutBox";

}
<div class="container">
    <div class="row vh-100 ">
        <div class="col-12 align-self-center">
            <div class="auth-page">
                <div class="card auth-card shadow-lg">
                    <div class="card-body">
                        <div class="px-3">
                            <div class="text-center auth-logo-text">
                                <h4 class="mt-0 mb-3 mt-5">Reset Password Fail</h4>
                            </div> <!--end auth-logo-text-->
                        </div><!--end /div-->
                        @if (ViewBag.ReturnCode != null)
                        {
                            @if (ViewBag.ReturnCode == ReturnCode.Error)
                            {
                                <div class="text-center">
                                    <span class="text-danger ">@ViewBag.Message</span>
                                </div>
                            }
                        }
                        <div class="m-3 text-center text-muted">
                            <a href="@Url.Action("Login","Auth")" class="btn btn-dark btn-block waves-effect waves-light" type="button">Back</a>
                        </div>
                    </div><!--end card-body-->
                </div><!--end card-->
            </div><!--end auth-page-->
        </div><!--end col-->
    </div><!--end row-->
</div>


